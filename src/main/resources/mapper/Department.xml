<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shu.db.dao.DepartmentDao">

    <select id="getDepartmentList" resultType="com.shu.db.entity.Department">
        SELECT * FROM d
    </select>

    <select id="getDepartmentByYxh" parameterType="java.lang.String" resultType="com.shu.db.entity.Department">
        SELECT  * FROM  d WHERE yxh=#{yxh}
    </select>

</mapper>